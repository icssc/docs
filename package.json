{
  "name": "docs.icssc.club",
  "version": "0.0.0",
  "description": "The official documentation site for ICSSC projects",
  "scripts": {
    "build": "next build",
    "build:worker": "opennextjs-cloudflare",
    "dev": "next dev",
    "dev:worker": "wrangler dev --port 8771",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "start": "next start",
    "preview:worker": "pnpm run build:worker && pnpm run dev:worker",
    "deploy:worker": "pnpm run build:worker && wrangler deploy",
    "postinstall": "fumadocs-mdx",
    "prepare": "husky"
  },
  "dependencies": {
    "fumadocs-core": "16.0.2",
    "fumadocs-mdx": "13.0.0",
    "fumadocs-twoslash": "3.1.9",
    "fumadocs-ui": "16.0.2",
    "lucide-react": "0.546.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@fumadocs/cli": "1.0.3",
    "@opennextjs/cloudflare": "1.11.0",
    "@types/mdx": "2.0.13",
    "@types/node": "22.18.12",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "autoprefixer": "10.4.21",
    "clsx": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "2.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.16",
    "typescript": "5.9.3",
    "wrangler": "4.45.0"
  },
  "packageManager": "pnpm@10.19.0"
}
