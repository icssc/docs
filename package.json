{
  "name": "docs.icssc.club",
  "version": "0.0.0",
  "description": "The official documentation site for ICSSC projects",
  "scripts": {
    "build": "next build",
    "build:worker": "opennextjs-cloudflare",
    "dev": "next dev",
    "dev:worker": "wrangler dev --port 8771",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "start": "next start",
    "preview:worker": "pnpm run build:worker && pnpm run dev:worker",
    "deploy:worker": "pnpm run build:worker && wrangler deploy",
    "postinstall": "fumadocs-mdx",
    "prepare": "husky"
  },
  "dependencies": {
    "fumadocs-core": "16.6.3",
    "fumadocs-mdx": "14.2.7",
    "fumadocs-twoslash": "3.1.13",
    "fumadocs-ui": "16.6.3",
    "lucide-react": "0.574.0",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-compare-slider": "3.1.0",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@commitlint/cli": "20.4.1",
    "@commitlint/config-conventional": "20.4.1",
    "@commitlint/types": "20.4.0",
    "@fumadocs/cli": "1.2.4",
    "@opennextjs/cloudflare": "1.16.5",
    "@types/mdx": "2.0.13",
    "@types/node": "22.10.7",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "autoprefixer": "10.4.24",
    "clsx": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "2.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "wrangler": "4.66.0"
  },
  "packageManager": "pnpm@10.30.0"
}
