{
  "name": "docs.icssc.club",
  "version": "0.0.0",
  "description": "The official documentation site for ICSSC projects",
  "scripts": {
    "build": "next build",
    "build:worker": "opennextjs-cloudflare",
    "dev": "next dev",
    "dev:worker": "wrangler dev --port 8771",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "start": "next start",
    "preview:worker": "pnpm run build:worker && pnpm run dev:worker",
    "deploy:worker": "pnpm run build:worker && wrangler deploy",
    "postinstall": "fumadocs-mdx",
    "prepare": "husky"
  },
  "dependencies": {
    "fumadocs-core": "14.7.5",
    "fumadocs-mdx": "11.3.1",
    "fumadocs-twoslash": "2.0.2",
    "fumadocs-ui": "14.7.5",
    "lucide-react": "0.473.0",
    "next": "14.2.21",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@commitlint/types": "19.5.0",
    "@fumadocs/cli": "0.0.7",
    "@opennextjs/cloudflare": "0.3.8",
    "@types/mdx": "2.0.13",
    "@types/node": "22.10.7",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "autoprefixer": "10.4.20",
    "clsx": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "2.3.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.1",
    "postcss": "8.5.1",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "wrangler": "3.103.2"
  },
  "packageManager": "pnpm@9.15.0"
}
