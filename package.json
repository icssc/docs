{
  "name": "docs.icssc.club",
  "version": "0.0.0",
  "description": "The official documentation site for ICSSC projects",
  "scripts": {
    "build": "next build",
    "build:worker": "cloudflare",
    "dev": "next dev",
    "dev:worker": "wrangler dev --port 8771",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "start": "next start",
    "preview:worker": "pnpm run build:worker && pnpm run dev:worker",
    "deploy:worker": "pnpm run build:worker && wrangler deploy",
    "postinstall": "fumadocs-mdx",
    "prepare": "husky"
  },
  "dependencies": {
    "fumadocs-core": "14.6.1",
    "fumadocs-mdx": "11.1.2",
    "fumadocs-twoslash": "2.0.2",
    "fumadocs-ui": "14.6.1",
    "lucide-react": "0.468.0",
    "next": "15.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@commitlint/types": "19.5.0",
    "@fumadocs/cli": "0.0.4",
    "@opennextjs/cloudflare": "0.2.1",
    "@types/mdx": "2.0.13",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "autoprefixer": "10.4.20",
    "clsx": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "2.3.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "postcss": "8.4.49",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2",
    "wrangler": "3.96.0"
  },
  "packageManager": "pnpm@9.15.0"
}
